{
  "name": "product-management-system-practice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:sass": "typed-scss-modules src",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "style:lint": "eslint --fix './src/**/*.{js,jsx,ts,tsx}'",
    "style:check": "prettier --check './src/**/*.{js,jsx,ts,tsx}'",
    "style:write": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "test": "eslint --fix -f table './src/**/*.ts' './src/**/*.tsx'",
    "pretty-quick": "pretty-quick",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "roules": "fireschema rules src/firebase/model/firestoreModel.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@tanstack/react-query": "^4.24.10",
    "@tanstack/react-query-devtools": "^4.24.12",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "firebase": "^9.18.0",
    "firebase-admin": "^11.0.0",
    "fireschema": "^5.0.0-36",
    "html-react-parser": "^3.0.8",
    "jotai": "^1.13.1",
    "next": "^13.1.2",
    "nookies": "^2.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.42.1",
    "react-intersection-observer": "^9.4.1",
    "type-fest": "^3.6.1",
    "use-breakpoint": "^3.0.6",
    "yup": "^0.32.11",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-formatter-table": "^7.32.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "framer-motion": "^8.4.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-node": "^10.9.1",
    "typed-scss-modules": "^7.0.2",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix -f table"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix"
    ]
  }
}
